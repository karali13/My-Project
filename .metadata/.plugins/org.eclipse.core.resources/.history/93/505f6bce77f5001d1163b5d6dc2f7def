package homework;

public class star1 {
	public static void main(String[] args) {
		for(int i=1; i<=3;i++) 
		{
			for(int j=i; j<=5;j++) 
			{
				
				for(int k=1; k<=(2*i)-6; k++)
				{
					System.out.print(" ");
				}
				
				
			System.out.print("*");
			}
			
			System.out.println();
		}
	}

}
